FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("libcatchyFiniteVolume.so");

application     cfdemSolverRhoPimple;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1.5;

deltaT          2e-5;

writeControl    adjustableRunTime;

writeInterval   0.05;

purgeWrite      0;

writeFormat     ascii;

writePrecision  15;

writeCompression uncompressed;

timeFormat      general;

timePrecision   7;

runTimeModifiable yes;

functions
{
    #includeFunc    "outletAverage"
    #includeFunc    "volAverage"
}
