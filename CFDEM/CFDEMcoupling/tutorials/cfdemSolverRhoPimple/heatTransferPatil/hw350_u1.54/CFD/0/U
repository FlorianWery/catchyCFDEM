FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    nozzle
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    inlet
    {
        type            interstitialUniformInletVelocity;
        //inletVelocity   constant (0 0 1.71);
        inletVelocity   table
        (
            (0.0 (0 0 0.01))
            (0.2 (0 0 1.71))
            (20.0 (0 0 1.71))
        );
        alpha           voidfraction;
        value           uniform (0 0 0.01);
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
}

// ************************************************************************* //
