FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      O2.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../system/caseSettings"

dimensions          [0 0 0 0 0 0 0];

internalField       uniform $Y_O2;

boundaryField
{
    inlet
    {
        type               fixedValue;
        value              uniform $Y_O2;
    }
    outlet
    {
        type               zeroGradient;
    }
    wall // group
    {
        type               zeroGradient;
    }
    cyclic  // group
    {
        type            cyclic;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
