FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0.2 0);
 
boundaryField
{
    reactingWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    inertWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            fixedValue;
        value           uniform (0 0.2 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    "(front|back)"
    {
        type            wedge;
    }
}


